{
  "name": "react-pc-template",
  "version": "1.0.0",
  "private": true,
  "description": "react pc",
  "keywords": [
    "react"
  ],
  "license": "MIT",
  "author": {
    "name": "lyk"
  },
  "scripts": {
    "build": "npm run build:prod",
    "build:pr": "cross-env NODE_ENV=production BUILD_ENV=pr webpack --config build/webpack.prod.config.js",
    "build:prod": "cross-env NODE_ENV=production BUILD_ENV=prod webpack --config build/webpack.prod.config.js",
    "build:int": "cross-env NODE_ENV=production BUILD_ENV=int webpack --config build/webpack.prod.config.js",
    "build:sit": "cross-env NODE_ENV=production BUILD_ENV=sit webpack --config build/webpack.prod.config.js",
    "build:uat": "cross-env NODE_ENV=production BUILD_ENV=uat webpack --config build/webpack.prod.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.config.js",
    "dll": "webpack --config build/webpack.dll.config.js -p --progress --profile",
    "lint": "eslint --fix src",
    "precommit": "lint-staged",
    "prettier": "prettier --write  \"./**/*.{js,jsx,css,less,md,json}\"",
    "start": "npm run dev",
    "test": "npm run lint"
  },
  "lint-staged": {
    "src/**/*{js,jsx}": [
      "prettier --write  \"./**/*.{js,jsx,css,less,md,json}\"",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/runtime": "^7.5.5",
    "antd": "^3.21.4",
    "axios": "^0.19.0",
    "braft-editor": "^2.3.9",
    "core-js": "^3.2.1",
    "crypto-js": "^4.0.0",
    "echarts": "^4.2.1",
    "es6-promise": "^4.2.8",
    "fastclick": "^1.0.6",
    "js-md5": "^0.7.3",
    "lrz": "^4.9.40",
    "md5": "^2.2.1",
    "moment": "^2.29.1",
    "qqmap": "^1.0.1",
    "rc-form": "^2.4.8",
    "react": "^16.9.0",
    "react-cropper": "^1.3.0",
    "react-dom": "^16.9.0",
    "react-intl-universal": "^2.1.3",
    "react-pdf-js": "^5.1.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-signature-canvas": "^1.0.3",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime-corejs3": "^7.5.5",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.1.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.3",
    "image-webpack-loader": "^6.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.2.1",
    "mini-css-extract-plugin": "^0.8.0",
